	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/josep11/puppeteer-botbase.git"
	},
	"scripts": {
		"start": "node index.js",
		"test": "NODE_ENV=test jest --testTimeout=50000 --runInBand tests/*",
		"test:tdd": "NODE_ENV=test jest --bail --colors --detectOpenHandles --detectLeaks --runInBand",
		"test:ci": "NODE_ENV=test jest --testPathIgnorePatterns \"tests/*botbase*\" \"tests/*helper-puppeteer*\" --detectLeaks --runInBand --reporters=default --reporters=jest-junit",
		"lint": "eslint . --ext .js,.ts",
		"lint:fix": "npm run lint -- --fix",
		"prebuild": "rm -rf dist/*",
		"build": "tsc -p tsconfig.base.json",
		"build-watch": "tsc --watch"
	},
	"dependencies": {
		"deepmerge": "^4.3.1",
		"fs": "0.0.1-security",
		"glob": "^10.3.10",
		"jest": "^29.7.0",
		"luxon": "^3.4.4",
		"user-agents": "^1.1.134"
	},
	"optionalDependencies": {
		"puppeteer-core": "^22.4.0"
	},
	"devDependencies": {
		"@types/chai": "^4.3.12",
		"@types/jest": "^29.5.12",
		"@types/luxon": "^3.4.2",
		"@types/node": "^20.11.24",
		"@types/user-agents": "^1.0.4",
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"chai": "^4.3.10",
		"dotenv": "^16.4.5",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"esm": "^3.2.25",
		"jest-junit": "^16.0.0",
		"puppeteer": "^22.4.0",
		"puppeteer-core": "^22.4.0",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	},
    "files": [
      "/dist"
    ]
}
