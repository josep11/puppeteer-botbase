{
	"name": "botbase",
	"version": "8.0.6",
	"description": "",
	"author": "Josep Alsina",
	"license": "ISC",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/josep11/puppeteer-botbase.git"
	},
	"scripts": {
		"start": "node index.js",
		"test": "NODE_ENV=test mocha --require esm --exit --colors --bail tests/*",
		"test:tdd": "NODE_ENV=test mocha --exit --colors --bail",
		"test:ci": "NODE_ENV=test mocha  --check-leaks --exit \"tests/!(*botbase*|*helper-puppeteer*).js\" --reporter mocha-junit-reporter --reporter-options mochaFile=./test-results/mocha/results.xml",
		"lint": "eslint . --ext .js,.ts",
		"lint:fix": "npm run lint -- --fix",
		"ncp": "ncp src dist",
		"prebuild": "rm -rf dist/* && npm run ncp",
		"build": "tsc",
		"build-watch": "tsc --watch"
	},
	"mocha": {
		"timeout": 55000
	},
	"dependencies": {
		"@types/jest": "^29.5.12",
		"deepmerge": "^4.3.1",
		"fs": "0.0.1-security",
		"glob": "^10.3.10",
		"jest": "^29.7.0",
		"luxon": "^3.4.4",
		"user-agents": "^1.1.124"
	},
	"optionalDependencies": {
		"aws-sdk": "^2.1564.0",
		"puppeteer-core": "^22.2.0"
	},
	"devDependencies": {
		"@types/chai": "^4.3.12",
		"@types/luxon": "^3.4.2",
		"@types/node": "^20.11.24",
		"@types/user-agents": "^1.0.4",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"chai": "^4.3.10",
		"dotenv": "^16.4.5",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"esm": "^3.2.25",
		"ncp": "^2.0.0",
		"puppeteer": "^22.2.0",
		"puppeteer-core": "^22.2.0",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	}
}
